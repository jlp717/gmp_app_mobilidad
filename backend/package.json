{
  "name": "gmp-app-unified-backend",
  "version": "2.0.0",
  "description": "Backend API para GMP App Unified - Granja Mari Pepa (Optimized)",
  "main": "server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "api": "ts-node src/api-server.ts",
    "build": "tsc",
    "start": "node server.js",
    "db:optimize": "node scripts/apply-db-optimizations.js",
    "start:pm2": "pm2 start ecosystem.config.js",
    "start:cluster": "pm2 start ecosystem.config.js -i max",
    "stop": "pm2 stop ecosystem.config.js",
    "restart": "pm2 restart ecosystem.config.js",
    "explore": "node db_explorer.js",
    "lint": "eslint src/**/*.ts --quiet",
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests",
    "test:watch": "jest --watch",
    "db:migrate": "sequelize-cli db:migrate",
    "db:seed": "sequelize-cli db:seed:all",
    "optimize:pipeline": "node scripts/fullOptimizationPipeline.js",
    "optimize:validate": "node scripts/validation/pre-post-validator.js validate",
    "optimize:baseline": "node scripts/validation/pre-post-validator.js baseline",
    "optimize:chaos": "node scripts/testing/chaos-engineering.js all",
    "optimize:rollback": "node scripts/rollback-manager.js rollback",
    "cache:cleanup": "node scripts/cache-cleanup.js",
    "flutter:test": "node scripts/flutterTestRunner.js"
  },
  "dependencies": {
    "@tensorflow/tfjs-node": "^4.15.0",
    "axios": "^1.13.2",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "node-schedule": "^2.1.1",
    "nodemailer": "^7.0.12",
    "odbc": "^2.4.8",
    "redis": "^4.6.11",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.16.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/hpp": "^0.2.5",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "Granja Mari Pepa",
  "license": "UNLICENSED"
}